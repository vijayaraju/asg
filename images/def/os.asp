'/** 
' * Operating system icon archive.
' * 
' * @param		(string) os 	- operating system name
' * @return 	(string) the encoded image associated with the os.
' * 
' * @author		Simone Carletti <carletti@weppos.net>
' */ 
public function archiveOS(os)
	
	Dim icon

	' Microsoft Windows
	if os = "Microsoft Windows 2003" then
		icon = ""
	elseif os = "Microsoft Windows CE" then
		icon = ""
	elseif os = "Microsoft Windows XP" then
		icon = ""
	elseif os = "Microsoft Windows 2000" then
		icon = ""
	' All NT versions
	elseif InStr(os, "Microsoft Windows NT") > 0 then
		icon = ""
	elseif os = "Microsoft Windows ME" then
		icon = ""
	' All 9x versions
	elseif InStr(os, "Microsoft Windows 9") > 0 then
		icon = ""
	elseif os = "Microsoft Windows 3.1" then
		icon = ""
	elseif InStr(os, "Microsoft Windows") > 0 then
		icon = ""
	
	' BeOS
	elseif os = "BeOS" then
		icon = ""
	
	' Mac OS
	elseif os = "Mac OS 10" then				' Mac 10 = Mac OS X
		icon = ""
	elseif InStr(os, "Mac OS") > 0 then		' Mac
		icon = ""
	elseif InStr(os, "Macintosh") > 0 then	' Common
		icon = ""
	
	' Linux
	elseif os = "Linux" then
		icon = ""
	
	' Solaris							' SUN OS is called Solaris
	elseif InStr(os, "Sun OS") > 0 then
		icon = ""
	' Sun OS
'	elseif InStr(os, "Sun OS") > 0 then
'		icon = "47494638396110001000910300FFFFFEA6CBF35D5DBEFFFFFF21F90401000003002C000000001000100000023A9C8FA926129B0408139E2085BDD9862751CFA05DC00434A268504D7A4E6A433A18754AF058C3FA8CF8E8500C8BC3B161DC928E4AB2557AF21005003B"

	' OS/2
	elseif os = "OS/2" then
		icon = ""
	
	' RiscOS
	elseif os = "RiscOS" then
		icon = ""
	
	' Amiga
	elseif InStr(os, "Amiga") > 0 then			' IRIX 64 was too big!
		icon = ""
	
	' FreeBSD
	elseif InStr(os, "FreeBSD") > 0 then
		icon = ""
	
	' NetBSD
	elseif InStr(os, "BSD") > 0 then
		icon = ""
	
	' Irix
	elseif InStr(os, "IRIX") > 0 then			' Irix 64 wa too big!
		icon = ""
	
	' HP-UX
	elseif os = "HP-UX" then
		icon = ""
	
	' AIX
	elseif InStr(os, "AIX") > 0 then
		icon = "47494638396110001000B30000E00514C5C5D400097A040F7E00117A0417730C7C40FEB809C0C0C000000000000000000000000000000000000000000021F90401000008002C000000001000100040043F10C949EB0C208373882102115800619AA3759DA97A1DAD8511DB61DCA27C7A5EECFE32E0AAE3A368381F832F40A371788342AC64DA2473BA5DB112E80A8511003B"
	
	' Symbian
	elseif os = "Symbian" then
		icon = ""
	
	' Unix
	elseif os = "UNIX" then
		icon = ""

	' Robots
	elseif InStr(os, "Robot") > 0 then
		icon = ""
	
	' SonyEricsson Mobile
	elseif InStr(os, "SonyEricsson Mobile") > 0 then
		icon = ""
		
	' Nokia Mobile
	elseif InStr(os, "Nokia Mobile") > 0 then
		icon = ""

	' ***** UNKNOWN *****

	elseif InStr(os, "(unknown)") > 0 then
		icon = ASG_ICON_EMPTY

	' ***** DEBUG *****

	' Undefined icons
	elseif os = "checkicon" then
		
		Const ASG_ICON_DEBUG_DOMAIN = "weppos.com"
		Const ASG_ICON_DEBUG_URL = "/asg/checkversion/check_icon.asp?"
		
		Dim osIcon
		osIcon = Session("osIcon")
		Session("osIcon") = ""
	
		' Use a cache to log icons just once
		if Len(osIcon) > 0 then
			Session("osIcon-" & Request.QueryString("page")) = "notified"
			Response.Redirect("http://" & ASG_ICON_DEBUG_DOMAIN &_
				ASG_ICON_DEBUG_URL & "iconos=" & osIcon &_
				"&host=" & Server.URLEncode(Request.ServerVariables("HTTP_HOST")))
		else
			icon = "4749463839610D000E00B30000FFFFFFF2F2F2EBEBEBE5E5E5DFDFDFD8D8D8D2D2D2CBCBCBC5C5C5BFBFBFB2B2B2A5A5A59F9F9F99999900000000000021F904041400FF002C000000000D000E0000043710C849AB9DC1DC2958DB40E17D57D19C45B86892791E00712AEDD924D2712EA18DE736848FA2B39D7E13991139192C3789136B23088022003B"
		end if

	' ***** ELSE *****

	else
		
		icon = ""
		
		' Add the engine to the unknown list
		Session("osIcon") = Session("osIcon") & os & "|"
	
	end if

	archiveOS = icon

end function

Dim strAsgIcon

' Print the icon
strAsgIcon = archiveOS(Request.QueryString("icon"))
printIcon(strAsgIcon)

%>